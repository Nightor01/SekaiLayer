<Window x:Class="SekaiLayer.UI.Windows.SettingsDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        mc:Ignorable="d"
        Icon="{StaticResource AppIcoIcon}"
        Title="Settings" Height="450" Width="300">
    <Grid>
        <GroupBox Header="Startup Settings">
            <CheckBox Checked="StartMinimizedCheckedStateChanged"
                      Unchecked="StartMinimizedCheckedStateChanged">
                Start minimized
            </CheckBox>
        </GroupBox>
    </Grid>
</Window>
