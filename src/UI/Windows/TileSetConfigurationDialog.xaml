<Window x:Class="SekaiLayer.UI.Windows.TileSetConfigurationDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:ctrls="clr-namespace:SekaiLayer.UI.Controls"
        mc:Ignorable="d"
        Title="TileSetConfigurationWindow" Height="450" Width="800"
        Icon="{StaticResource AppIcoIcon}"
        DataContext="{Binding RelativeSource={RelativeSource Self}}">
    <Grid>
        <ctrls:TileSetConfigurationControl Image="{Binding Image, Mode=OneWay}"
                                           x:Name="TileSetConfigurationControl" />
    </Grid>
</Window>
